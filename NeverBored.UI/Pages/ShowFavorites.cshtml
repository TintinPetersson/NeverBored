@page
@model NeverBored.UI.Pages.ShowFavoritesModel
@{
}
<div class="container text-light">
    <div class="row justify-content-center text-light">
        <div class="col-4 text-light">
            @if (Model.FavoritesList != null)
            {
                @foreach (var activity in Model.FavoritesList)
                {
                    <p class="text-light">@activity.Activity</p>
                }
            }
        </div>
    </div>
</div>
